<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.kerner1000.drrename.GoCancelButtonsComponentController" spacing="4">
    <Button fx:id="buttonGo" prefWidth="70" minWidth="50"
            onAction="#handleDummyFileCreatorButtonGo" text="Go">
        <tooltip>
            <Tooltip text="Create files"/>
        </tooltip>
    </Button>
    <Button fx:id="buttonCancel" prefWidth="70" minWidth="50"
            onAction="#handleDummyFileCreatorButtonCancel" text="Cancel">
        <tooltip>
            <Tooltip text="Create files"/>
        </tooltip>
    </Button>
</HBox>
